{
  "expo": {
    "name": "LS Chat",
    "slug": "ls-chat",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/mosh-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kbgutierrez.lschat",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleDisplayName": "LS Chat"
      },
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "icon": "./assets/images/mosh-icon.png",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.CAMERA"
      ],
      "runtimeVersion": "1.0.0",
      "package": "com.kbgutierrez.lschat"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "meta": {
        "viewport": "width=device-width, initial-scale=1, maximum-scale=1"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-sqlite",
        {
          "enableFTS": true,
          "useSQLCipher": false,
          "android": {},
          "ios": {}
        }
      ],
      "expo-barcode-scanner",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/mosh-icon.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/notification_sound.wav"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e9631479-dfc2-48a9-a210-612f5d595e98"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/e9631479-dfc2-48a9-a210-612f5d595e98",
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    
    },
    "owner": "kbgutierrez"
  }
}
