{
  "expo": {
    "name": "LS Chat",
    "slug": "ls-chat",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/logo/ls_chat4.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kbgutierrez.lschat",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleDisplayName": "LS Chat",
        "UIBackgroundModes": [
          "fetch",
          "remote-notification"
        ]
      },
      "buildNumber": "1",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "additionalProperties": {
        "googleServices": true
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0a0d54"
      },
      "icon": "./assets/images/adaptive-icon.png",
      "permissions": [
        "NOTIFICATIONS",
        "RECEIVE_BOOT_COMPLETED",
        "SCHEDULE_EXACT_ALARM",
        "WAKE_LOCK"
      ],
      "runtimeVersion": "1.0.0",
      "versionCode": 1,
      "package": "com.kbgutierrez.lschat",
      "networkSecurityConfig": {
        "cleartextTrafficPermitted": true
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "meta": {
        "viewport": "width=device-width, initial-scale=1, maximum-scale=1"
      }
    },
    "plugins": [
      "expo-router",
      "expo-splash-screen",
      "expo-sqlite",
      "expo-barcode-scanner",
      "expo-notifications",
      "expo-asset",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/crashlytics",
      "@react-native-firebase/messaging"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "url": "https://u.expo.dev/e9631479-dfc2-48a9-a210-612f5d595e98",
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "owner": "ls_chat_group",
    "splash": {
      "image": "./assets/images/adaptive-icon.png",
      "backgroundColor": "#8a2be2",
      "resizeMode": "contain"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e9631479-dfc2-48a9-a210-612f5d595e98"
      }
    }
  }
}
